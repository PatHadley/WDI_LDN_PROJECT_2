<% @users.each do |user| %>
<ul class="list-group">
  <% if user.available == true && user != current_user %>
  <li class="list-group-item">
  <% if user.profile_pic.length > 1 %>
  <%= image_tag(user.profile_pic, class: "img-responsive img-circle") %>
  <% else %>
    <%= fallback_img %>
  <% end %>
  <span class="h4">
    <%= link_to "#{user.username}", user_display_path(user) %></span>
  </li>
  <% end %>
</ul>
<% end %>